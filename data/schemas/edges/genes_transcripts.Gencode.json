{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "id": "schemas/edges/genes_transcripts.Gencode.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "The unique key of the gene transcript edge",
            "example": "ENSG00000290825_ENST00000456328 or ENSMUSG00000026805_ENSMUST00000113847"
        },
        "_from": {
            "type": "string",
            "description": "The from node of the gene transcript edge",
            "example": "genes/ENSG00000290825 or mm_genes/ENSMUSG00000026805",
            "pattern": "^(genes/ENSG[0-9]{11}(?:_PAR_Y)?|mm_genes/ENSMUSG[0-9]{11}(?:_PAR_Y)?)$",
            "collections": ["genes", "mm_genes"]
        },
        "_to": {
            "type": "string",
            "description": "The to node of the gene transcript edge",
            "example": "transcripts/ENST00000618998 or mm_transcripts/ENSMUST00000113847",
            "pattern": "^(transcripts/ENST[0-9]{11}(?:_PAR_Y)?|mm_transcripts/ENSMUST[0-9]{11}(?:_PAR_Y)?)$",
            "collections": ["transcripts", "mm_transcripts"]
        },
        "source": {
            "type": "string",
            "description": "The source of the gene transcript edge",
            "enum": ["GENCODE"]
        },
        "version": {
            "type": "string",
            "description": "The version of the gene transcript edge",
            "enum": ["v43", "vM36"]
        },
        "source_url": {
            "type": "string",
            "description": "The source URL of the gene transcript edge",
            "example": "https://www.gencodegenes.org/human/release_43/gencode.v43.annotation.gff3.gz"
        },
        "name": {
            "type": "string",
            "description": "The name of the gene transcript edge",
            "enum": ["transcribes"]
        },
        "inverse_name": {
            "type": "string",
            "description": "The inverse name of the gene transcript edge",
            "enum": ["transcribed by"]
        },
        "organism": {
            "type": "string",
            "description": "The organism of the gene transcript edge",
            "enum": ["Homo sapiens", "Mus musculus"]
        },
        "biological_process": {
            "type": "string",
            "description": "The biological process of the gene transcript edge",
            "enum": ["ontology_terms/GO_0010467"]
        }
    },
    "required": ["_key", "_from", "_to", "source", "version", "source_url", "name", "inverse_name", "organism", "biological_process"],
    "additionalProperties": false
}
