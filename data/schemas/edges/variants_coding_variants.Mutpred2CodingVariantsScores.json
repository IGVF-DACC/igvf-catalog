{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/variants_coding_variants.Mutpred2CodingVariantsScores.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the variant-coding variant edge"
        },
        "_from": {
          "example": "variants/NC_000001.11:10202:C:A",
          "pattern": "^variants/",
          "collections": [
            "variants"
          ]
        },
        "_to": {
          "example": "coding_variants/OR4F5_ENST00000641515_p.Met1!_c.1A-C",
          "pattern": "^coding_variants/",
          "collections": [
            "coding_variants"
          ]
        },
        "name": {
          "enum": [
            "codes for"
          ]
        },
        "inverse_name": {
          "enum": [
            "encoded by"
          ]
        },
        "chr": {
          "type": "string",
          "description": "Chromosome for the variant-coding variant edge",
          "example": "chr18"
        },
        "pos": {
          "type": "integer",
          "description": "Position for the variant-coding variant edge"
        },
        "ref": {
          "type": "string",
          "description": "Reference allele for the variant-coding variant edge",
          "example": "CA"
        },
        "alt": {
          "type": "string",
          "description": "Alternate allele for the variant-coding variant edge",
          "example": "AC"
        },
        "method": {
          "enum": [
            "MutPred2"
          ]
        },
        "class": {
          "enum": [
            "prediction"
          ]
        },
        "label": {
          "enum": [
            "codes for"
          ]
        },
        "biosample_term": {
          "type": ["string", "null"],
          "description": "Biosample term for the variant-coding variant edge",
          "pattern": "^ontology_terms/"
        },
        "biological_context": {
          "type": ["string", "null"],
          "description": "Biological context for the variant-coding variant edge",
          "example": "glutamatergic neuron differentiated cell specimen, pooled cell"
        },
        "source": {
          "enum": [
            "IGVF"
          ]
        },
        "source_url": {
          "enum": [
            "https://data.igvf.org/tabular-files/IGVFFI6893ZOAA"
          ]
        }
      },
      "required": [
        "_key",
        "chr",
        "pos",
        "ref",
        "alt",
        "biosample_term",
        "biological_context"

      ],
      "additionalProperties": false
    }
  ],
  "db_collection_name": "variants_coding_variants"
}
