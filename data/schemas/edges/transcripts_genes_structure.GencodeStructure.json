{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "schemas/edges/transcripts_genes_structure.GencodeStructure.json",
    "type": "object",
    "properties": {
        "_from": {
            "type": "string",
            "description": "The from node of the gene structure",
            "example": "transcripts/ENST00000456328",
            "pattern": "^transcripts/ENST[0-9]{11}(?:_PAR_Y)?$",
            "collections": ["transcripts"]
        },
        "_to": {
            "type": "string",
            "description": "The to node of the gene structure",
            "example": "genes_structure/ENSG00000290825_ENSE00002234944_exon",
            "pattern": "^genes_structure/",
            "collections": ["genes_structure"]
        },
        "source": {
            "type": "string",
            "description": "The source of the gene structure",
            "enum": ["GENCODE"]
        },
        "version": {
            "type": "string",
            "description": "The version of the gene structure annotation",
            "enum": ["v43"]
        },
        "source_url": {
            "type": "string",
            "description": "The source URL of the gene structure annotation",
            "enum": ["https://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_43/gencode.v43.chr_patch_hapl_scaff.annotation.gtf.gz"]
        },
        "organism": {
            "type": "string",
            "description": "The organism of the gene structure",
            "enum": ["Homo sapiens"]
        },
        "name": {
            "type": "string",
            "description": "The name of the gene structure",
            "enum": ["contains"]
        },
        "inverse_name": {
            "type": "string",
            "description": "The inverse name of the gene structure",
            "enum": ["contained in"]
        }
    },
    "required": ["_from", "_to", "source", "version", "source_url", "organism", "name", "inverse_name"],
    "additionalProperties": false
}
