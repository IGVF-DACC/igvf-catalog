{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/variants_phenotypes.cV2F.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the variant to phenotype edge",
          "example": "NC_000001.11:91420:T:C_GO_0003674_IGVFFI0332UGDD"
        },
        "_from": {
          "example": "variants/NC_000001.11:91420:T:C",
          "pattern": "^variants/",
          "collections": [
            "variants"
          ]
        },
        "_to": {
          "example": "ontology_terms/GO_0003674",
          "pattern": "^ontology_terms/",
          "collections": [
            "ontology_terms"
          ]
        },
        "score": {
          "type": "number",
          "description": "Score for the variant to phenotype edge"
        },
        "source": {
          "enum": [
            "IGVF"
          ]
        },
        "source_url": {
          "example": "https://data.igvf.org/tabular-files/IGVFFI0332UGDD",
          "pattern": "^https://data.igvf.org/tabular-files/"
        },
        "name": {
          "enum": [
            "associated with"
          ]
        },
        "inverse_name": {
          "enum": [
            "associated with"
          ]
        },
        "files_filesets": {
          "type": "string",
          "description": "Filesets for the variant to phenotype edge",
          "example": "files_filesets/IGVFFI0332UGDD",
          "pattern": "^files_filesets/",
          "collections": [
            "files_filesets"
          ]
        },
        "biosample_term": {
          "type": [
            "string",
            "null"
          ],
          "description": "Biosample term for the variant to phenotype edge",
          "example": "ontology_terms/UBERON_0002048",
          "pattern": "^ontology_terms/"
        },
        "method": {
          "enum": [
            "cV2F"
          ]
        },
        "class": {
          "enum": [
            "prediction"
          ]
        },
        "label": {
          "enum": [
            "predicted variant effect on phenotype"
          ]
        },
        "biological_context": {
          "type": [
            "string",
            "null"
          ],
          "description": "Biological context for the variant to phenotype edge",
          "example": "lung"
        }
      },
      "required": [
        "_key",
        "score",
        "files_filesets",
        "biosample_term",
        "biological_context"
      ]
    }
  ],
  "additionalProperties": false,
  "db_collection_name": "variants_phenotypes"
}
