{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/genomic_elements_genes.EncodeElementGeneLink.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the genomic element to gene edge",
          "example": "intergenic_chr1_778465_778965_GRCh38_ENSG00000225880_ENCFF003BKC"
        },
        "_from": {
          "example": "genomic_elements/intergenic_chr1_778465_778965_GRCh38_ENSG00000225880_ENCFF003BKC",
          "pattern": "^genomic_elements/",
          "collections": [
            "genomic_elements"
          ]
        },
        "_to": {
          "example": "genes/ENSG00000225880",
          "pattern": "^genes/",
          "collections": [
            "genes"
          ]
        },
        "method": {
          "enum": [
            "ENCODE-rE2G"
          ]
        },
        "label": {
          "enum": [
            "predicted regulatory element effect on gene expression"
          ]
        },
        "class": {
          "enum": [
            "prediction"
          ]
        },
        "score": {
          "type": "number",
          "description": "The score of the genomic element to gene edge"
        },
        "source": {
          "enum": [
            "ENCODE"
          ]
        },
        "source_url": {
          "example": "https://www.encodeproject.org/files/ENCFF003BKC/",
          "pattern": "^https://www.encodeproject.org/files/"
        },
        "files_filesets": {
          "type": "string",
          "description": "The filesets of the genomic element to gene edge",
          "example": "files_filesets/ENCFF003BKC",
          "pattern": "^files_filesets/"
        },
        "biological_context": {
          "type": "string",
          "description": "The biological context of the genomic element to gene edge",
          "example": "K562"
        },
        "biosample_term": {
          "type": "string",
          "description": "The biosample term of the genomic element to gene edge",
          "example": "ontology_terms/EFO_0002067",
          "pattern": "^ontology_terms/"
        },
        "treatments_term_ids": {
          "type": [
            "array",
            "null"
          ],
          "description": "The treatments term ids of the genomic element to gene edge",
          "example": [
            "UniProtKB:P05112"
          ]
        },
        "name": {
          "enum": [
            "regulates"
          ]
        },
        "inverse_name": {
          "enum": [
            "regulated by"
          ]
        }
      },
      "required": [
        "_key",
        "score",
        "files_filesets",
        "biological_context",
        "biosample_term",
        "treatments_term_ids"
      ],
      "additionalProperties": false
    }
  ],
  "db_collection_name": "genomic_elements_genes"
}
