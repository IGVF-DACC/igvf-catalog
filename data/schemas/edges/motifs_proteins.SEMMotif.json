{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/edges/motifs_proteins.SEMMotif.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "Unique identifier for the motif protein edge"
        },
        "_from": {
            "type": "string",
            "description": "The from node of the motif protein edge",
            "pattern": "^motifs/",
            "collections": ["motifs"]
        },
        "_to": {
            "type": "string",
            "description": "The to node of the motif protein edge",
            "pattern": "^proteins/",
            "collections": ["proteins"]
        },
        "name": {
            "type": "string",
            "description": "The name of the motif protein edge",
            "enum": ["is used by"]
        },
        "inverse_name": {
            "type": "string",
            "description": "The inverse name of the motif protein edge",
            "enum": ["uses"]
        },
        "biological_process": {
            "type": "string",
            "description": "The biological process of the motif protein edge",
            "enum": ["ontology_terms/GO_0003677"]
        },
        "source": {
            "type": "string",
            "description": "The source of the motif protein edge",
            "enum": ["IGVF"]
        },
        "source_url": {
            "type": "string",
            "description": "The source url of the motif protein edge",
            "example": "https://data.igvf.org/model-files/IGVFFI9967LYCS",
            "pattern": "^https://data.igvf.org/model-files/"
        }
    },
    "required": ["_key", "_from", "_to", "name", "inverse_name", "biological_process", "source", "source_url"],
    "additionalProperties": false
}
