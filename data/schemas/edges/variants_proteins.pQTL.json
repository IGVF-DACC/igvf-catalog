{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/edges/variants_proteins.pQTL.json",
    "allOf": [
        {
            "$ref": "edges.base.json"
        },
        {
            "type": "object",
            "properties": {
            "_key": {
                "type": "string",
                "description": "Unique identifier for the variant to protein edge",
                "example": "NC_000017.11:7166092:G:A_ENSP00000491556_UKB"
            },
            "_from": {
                "example": "variants/NC_000001.11:10000:T:A",
                "pattern": "^variants/",
                "collections": ["variants"]
            },
            "_to": {
                "example": "proteins/ENSP00000456328",
                "pattern": "^proteins/",
                "collections": ["proteins"]
            },
            "rsid": {
                "type": ["string", "null"],
                "description": "The rsid of the variant to protein edge",
                "example": "rs1260326"
            },
            "label": {
                "enum": ["pQTL"]
            },
            "log10pvalue": {
                "type": "number",
                "description": "The log10pvalue of the variant to protein edge"
            },
            "beta": {
                "type": "number",
                "description": "The beta of the variant to protein edge"
            },
            "se": {
                "type": "number",
                "description": "The se of the variant to protein edge"
            },
            "class": {
                "enum": ["cis", "trans"]
            },
            "gene": {
                "type": ["string", "null"],
                "description": "The gene of the variant to protein edge",
                "example": "genes/ENSG00000165841"
            },
            "gene_consequence": {
                "type": ["string", "null"],
                "description": "The gene consequence of the variant to protein edge",
                "example": "intergenic_variant"
            },
            "biological_context": {
                "type": "string",
                "description": "The biological context of the variant to protein edge",
                "enum": ["blood plasma"]
            },
            "source": {
                "enum": ["UKB"]
            },
            "source_url": {
                "enum": ["https://metabolomips.org/ukbbpgwas/"]
            },
            "name": {
                "enum": ["associated with levels of"]
            },
            "inverse_name": {
                "enum": ["level associated with"]
            },
            "method": {
                "enum": ["ontology_terms/BAO_0080027"]
            }
            },
            "required": ["_key",  "rsid", "log10pvalue", "beta", "se",  "gene", "gene_consequence", "biological_context"],
            "additionalProperties": false
        }
    ]
}
