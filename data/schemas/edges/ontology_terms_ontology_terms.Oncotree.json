{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/edges/ontology_terms_ontology_terms.Oncotree.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "Unique identifier for the ontology term to ontology term edge",
            "example": "Oncotree_ASM_rdf-schema.subClassOf_Oncotree_SM"
        },
        "_from": {
            "type": "string",
            "description": "Source for the ontology term to ontology term edge",
            "example": "ontology_terms/Oncotree_ASM",
            "pattern": "^ontology_terms/Oncotree_",
            "collections": ["ontology_terms"]
        },
        "_to": {
            "type": "string",
            "description": "Target for the ontology term to ontology term edge",
            "example": "ontology_terms/Oncotree_SM or ontology_terms/NCIT_C40511",
            "pattern": "^ontology_terms/Oncotree_|^ontology_terms/NCIT_",
            "collections": ["ontology_terms"]
        },
        "name": {
            "type": "string",
            "description": "Name for the ontology term to ontology term edge",
            "enum": ["subclass", "database cross-reference"]
        },
        "inverse_name": {
            "type": "string",
            "description": "Inverse name for the ontology term to ontology term edge",
            "enum": ["type of", "database cross-reference"]
        },
        "type": {
            "type": "string",
            "description": "Type for the ontology term to ontology term edge",
            "enum": ["subclass", "database cross-reference"]
        },
        "source": {
            "type": "string",
            "description": "Source for the ontology term to ontology term edge",
            "enum": ["Oncotree"]
        }
    },
    "required": ["_key", "_from", "_to", "name", "inverse_name", "type", "source"],
    "additionalProperties": false
}
