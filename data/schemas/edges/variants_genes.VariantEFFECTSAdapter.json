{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/variants_genes.VariantEFFECTSAdapter.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the variant to gene edge"
        },
        "_from": {
          "example": "variants/NC_000001.11:10000:T:A",
          "pattern": "^variants/",
          "collections": [
            "variants"
          ]
        },
        "_to": {
          "example": "genes/ENSG00000187583.10",
          "pattern": "^genes/ENSG[0-9]{11}(?:_PAR_Y)?$",
          "collections": [
            "genes"
          ]
        },
        "effect_size": {
          "type": "number",
          "description": "Effect size for the variant to gene edge"
        },
        "log2_fold_change": {
          "type": "number",
          "description": "Log2 fold change for the variant to gene edge"
        },
        "p_nominal_nlog10": {
          "type": "number",
          "description": "P nominal nlog10 for the variant to gene edge"
        },
        "fdr_nlog10": {
          "type": "number",
          "description": "FDR nlog10 for the variant to gene edge"
        },
        "power": {
          "type": [
            "number",
            "null"
          ],
          "description": "Power for the variant to gene edge"
        },
        "class": {
          "enum": [
            "observed data"
          ]
        },
        "label": {
          "enum": [
            "variant effect on gene expression"
          ]
        },
        "name": {
          "description": "Name for the variant to gene edge",
          "enum": [
            "modulates expression of"
          ]
        },
        "inverse_name": {
          "description": "Inverse name for the variant to gene edge",
          "enum": [
            "expression modulated by"
          ]
        },
        "source": {
          "description": "Source for the variant to gene edge",
          "enum": [
            "IGVF"
          ]
        },
        "source_url": {
          "description": "Source URL for the variant to gene edge",
          "example": "https://data.igvf.org/tabular-files/IGVFFI0524YUIL/"
        },
        "files_filesets": {
          "type": "string",
          "description": "Filesets for the variant to gene edge",
          "example": "files_filesets/IGVFFI0332UGDD",
          "pattern": "^files_filesets/"
        },
        "method": {
          "description": "Method for the variant to gene edge",
          "enum": [
            "Variant-EFFECTS"
          ]
        },
        "biological_context": {
          "type": "string",
          "description": "Biological context for the variant to gene edge",
          "items": {
            "type": "string"
          },
          "example": "THP-1"
        },
        "biosample_term": {
          "type": "string",
          "description": "Biosample term for the variant to gene edge",
          "example": "ontology_terms/EFO_0002067",
          "pattern": "^ontology_terms/"
        },
        "treatments_term_ids": {
          "type": [
            "array",
            "null"
          ],
          "description": "Treatments term IDs for the variant to gene edge",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "_key",
        "effect_size",
        "log2_fold_change",
        "p_nominal_nlog10",
        "fdr_nlog10",
        "power",
        "label",
        "source_url",
        "files_filesets",
        "biological_context",
        "biosample_term",
        "treatments_term_ids"
      ],
      "additionalProperties": false
    }
  ],
  "db_collection_name": "variants_genes"
}
