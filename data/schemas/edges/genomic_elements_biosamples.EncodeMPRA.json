{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/genomic_elements_biosamples.EncodeMPRA.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the genomic element to biosample edge",
          "example": "MPRA_chr15_85766445_85766645_GRCh38_ENCFF769REH_EFO_0009747"
        },
        "_from": {
          "example": "genomic_elements/MPRA_chr15_85766445_85766645_GRCh38_ENCFF769REH",
          "pattern": "^genomic_elements/",
          "collections": [
            "genomic_elements"
          ]
        },
        "_to": {
          "example": "ontology_terms/EFO_0009747",
          "pattern": "^ontology_terms/",
          "collections": [
            "ontology_terms"
          ]
        },
        "element_name": {
          "type": "string",
          "description": "The name of the genomic element to biosample edge",
          "example": "WTC11_seq26582_F"
        },
        "strand": {
          "type": "string",
          "description": "The strand of the genomic element to biosample edge",
          "example": "+"
        },
        "activity_score": {
          "type": "number",
          "description": "The activity score of the genomic element to biosample edge"
        },
        "bed_score": {
          "type": "integer",
          "description": "The bed score of the genomic element to biosample edge"
        },
        "DNA_count": {
          "type": "number",
          "description": "The DNA count of the genomic element to biosample edge"
        },
        "RNA_count": {
          "type": "number",
          "description": "The RNA count of the genomic element to biosample edge"
        },
        "method": {
          "enum": [
            "MPRA"
          ]
        },
        "class": {
          "enum": [
            "observed data"
          ]
        },
        "label": {
          "enum": [
            "regulatory element activity"
          ]
        },
        "biological_context": {
          "type": "string",
          "description": "The biological context of the genomic element to biosample edge",
          "example": "K562"
        },
        "biosample_term": {
          "type": "string",
          "description": "The biosample term of the genomic element to biosample edge",
          "example": "ontology_terms/EFO_0002067"
        },
        "source": {
          "enum": [
            "ENCODE"
          ]
        },
        "source_url": {
          "example": "https://www.encodeproject.org/files/ENCFF802FUV/",
          "pattern": "^https://www.encodeproject.org/files/"
        },
        "files_filesets": {
          "type": "string",
          "description": "The filesets of the genomic element to biosample edge",
          "example": "files_filesets/ENCFF802FUV",
          "pattern": "^files_filesets/"
        },
        "name": {
          "enum": [
            "expression effect in"
          ]
        },
        "inverse_name": {
          "enum": [
            "has expression effect from"
          ]
        }
      },
      "required": [
        "_key",
        "element_name",
        "strand",
        "activity_score",
        "bed_score",
        "biological_context",
        "biosample_term",
        "DNA_count",
        "RNA_count",
        "files_filesets"
      ],
      "additionalProperties": false
    }
  ],
  "accessible_via": {
    "return": "_id, element_name, strand, activity_score, bed_score, DNA_count, RNA_count, source, source_url"
  },
  "db_collection_name": "genomic_elements_biosamples"
}
