{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "id": "schemas/edges/genes_genes.GeneGeneBiogrid.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "The unique key of the gene gene edge"
        },
        "_from": {
            "type": "string",
            "description": "The from node of the gene gene edge",
            "example": "mm_genes/ENSMUSG00000037526",
            "pattern": "^mm_genes/ENSMUSG[0-9]{11}(?:_PAR_Y)?$",
            "collections": ["mm_genes"]
        },
        "_to": {
            "type": "string",
            "description": "The to node of the gene gene edge",
            "example": "mm_genes/ENSMUSG00000037526",
            "pattern": "^mm_genes/ENSMUSG[0-9]{11}(?:_PAR_Y)?$",
            "collections": ["mm_genes"]
        },
        "detection_method": {
            "type": "string",
            "description": "The detection method of the gene gene edge",
            "enum": ["genetic interference"]
        },
        "detection_method_code": {
            "type": "string",
            "description": "The detection method code of the gene gene edge",
            "enum": ["MI:0254"]
        },
        "interaction_type": {
            "type": "array",
            "description": "The interaction type of the gene gene edge",
            "items": {
                "type": "string",
                "enum": [
                    "dosage rescue (sensu BioGRID)",
                    "negative genetic interaction (sensu BioGRID)",
                    "phenotypic enhancement (sensu BioGRID)",
                    "phenotypic suppression (sensu BioGRID)",
                    "positive genetic interaction (sensu BioGRID)",
                    "synthetic growth defect (sensu BioGRID)",
                    "synthetic haploinsufficiency (sensu BioGRID)",
                    "synthetic lethality (sensu BioGRID)",
                    "synthetic rescue (sensu BioGRID)"
                  ]
            }
        },
        "interaction_type_code": {
            "type": "array",
            "description": "The interaction type code of the gene gene edge",
            "items": {
                "type": "string",
                "enum": [
                    "MI:2368",
                    "MI:2369",
                    "MI:2370",
                    "MI:2371",
                    "MI:2372",
                    "MI:2373",
                    "MI:2374",
                    "MI:2375",
                    "MI:2376"
                  ]
            }
        },
        "confidence_value_biogrid": {
            "type": ["number", "null"],
            "description": "The confidence value of the gene gene edge"
        },
        "confidence_value_intact": {
            "type": ["number", "null"],
            "description": "The confidence value of the gene gene edge"
        },
        "source": {
            "type": "string",
            "description": "The source of the gene gene edge",
            "enum": ["BioGRID"]
        },
        "pmids": {
            "type": "array",
            "description": "The pmids of the gene gene edge",
            "example": [
                "http://pubmed.ncbi.nlm.nih.gov/33660365"
            ],
            "items": {
                "type": "string"
            }
        },
        "z_score": {
            "type": "number",
            "description": "The z score of the gene gene edge",
            "enum": [0]
        },
        "name": {
            "type": "string",
            "description": "The name of the gene gene edge",
            "enum": ["interacts with"]
        },
        "inverse_name": {
            "type": "string",
            "description": "The inverse name of the gene gene edge",
            "enum": ["interacts with"]
        },
        "molecular_function": {
            "type": "string",
            "description": "The molecular function of the gene gene edge",
            "enum": ["ontology_terms/GO_0005515"]
        }
    },
    "required": ["_key", "_from", "_to", "detection_method", "detection_method_code", "interaction_type", "interaction_type_code", "confidence_value_biogrid", "confidence_value_intact", "source", "pmids", "z_score", "name", "inverse_name", "molecular_function"],
    "additionalProperties": false
}
