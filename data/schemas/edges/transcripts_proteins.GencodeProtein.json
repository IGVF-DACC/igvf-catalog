{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "schemas/edges/transcripts_proteins.GencodeProtein.json",
    "type": "object",
    "properties": {
        "_from": {
            "type": "string",
            "description": "The from node of the transcript protein edge",
            "example": "transcripts/ENST00000456328 or transcripts/ENSMUST00000456328",
            "pattern": "^transcripts/ENST[0-9]{11}(?:_PAR_Y)?$|transcripts/ENSMUST[0-9]{11}(?:_PAR_Y)?$",
            "collections": ["transcripts", "mm_transcripts"]
        },
        "_to": {
            "type": "string",
            "description": "The to node of the transcript protein edge",
            "example": "proteins/ENSP00000456328",
            "pattern": "^proteins/ENSP[0-9]{11}$",
            "collections": ["proteins"]
        },
        "source": {
            "type": "string",
            "description": "The source of the transcript protein edge",
            "enum": ["GENCODE"]
        },
        "version": {
            "type": "string",
            "description": "The version of the transcript protein edge",
            "enum": ["v43", "vM36"]
        },
        "source_url": {
            "type": "string",
            "description": "The source URL of the transcript protein edge",
            "enum": ["https://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_43/gencode.v43.chr_patch_hapl_scaff.annotation.gtf.gz", "https://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_mouse/release_M36/gencode.vM36.chr_patch_hapl_scaff.annotation.gtf.gz"]
        },
        "organism": {
            "type": "string",
            "description": "The organism of the transcript protein edge",
            "enum": ["Homo sapiens", "Mus musculus"]
        },
        "name": {
            "type": "string",
            "description": "The name of the transcript protein edge",
            "enum": ["translates to"]
        },
        "inverse_name": {
            "type": "string",
            "description": "The inverse name of the transcript protein edge",
            "enum": ["translated from"]
        },
        "biological_process": {
            "type": "string",
            "description": "The biological process of the transcript protein edge",
            "enum": ["ontology_terms/GO_0006412"]
        }

    },
    "required": ["_from", "_to", "source", "version", "source_url", "organism", "name", "inverse_name", "biological_process"],
    "additionalProperties": false
}
