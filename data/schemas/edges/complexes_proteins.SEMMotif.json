{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/edges/complexes_proteins.SEMMotif.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "Unique identifier for the complexes to proteins edge"
        },
        "_from": {
            "type": "string",
            "description": "The from node of the complexes to proteins edge",
            "pattern": "^complexes/SEMpl_",
            "collections": ["complexes"]
        },
        "_to": {
            "type": "string",
            "description": "The to node of the complexes to proteins edge",
            "pattern": "^proteins/",
            "collections": ["proteins"]
        },
        "name": {
            "type": "string",
            "description": "The name of the complexes to proteins edge",
            "enum": ["contains"]
        },
        "inverse_name": {
            "type": "string",
            "description": "The inverse name of the complexes to proteins edge",
            "enum": ["belongs to"]
        },
        "source": {
            "type": "string",
            "description": "The source of the complexes to proteins edge",
            "enum": ["IGVF"]
        },
        "source_url": {
            "type": "string",
            "description": "The source url of the complexes to proteins edge",
            "example": "https://data.igvf.org/model-files/IGVFFI9967LYCS",
            "pattern": "^https://data.igvf.org/model-files/"
        }
    },
    "required": ["_key", "_from", "_to", "name", "inverse_name", "source", "source_url"],
    "additionalProperties": false
}
