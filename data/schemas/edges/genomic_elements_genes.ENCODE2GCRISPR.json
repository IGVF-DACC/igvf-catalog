{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/genomic_elements_genes.ENCODE2GCRISPR.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the genomic element to gene edge",
          "example": "CRISPR_chr1_3774714_3775214_GRCh38_ENSG00000116198_ENCFF968BZL"
        },
        "_from": {
          "example": "genomic_elements/CRISPR_chr1_3774714_3775214_GRCh38_ENCFF968BZL",
          "pattern": "^genomic_elements/",
          "collections": [
            "genomic_elements"
          ]
        },
        "_to": {
          "example": "genes/ENSG00000116198",
          "pattern": "^genes/",
          "collections": [
            "genes"
          ]
        },
        "score": {
          "type": "number",
          "description": "The score of the genomic element to gene edge"
        },
        "p_value": {
          "type": "number",
          "description": "The p value of the genomic element to gene edge"
        },
        "log10pvalue": {
          "type": "number",
          "description": "The log10pvalue of the genomic element to gene edge"
        },
        "significant": {
          "type": "boolean",
          "description": "The significant of the genomic element to gene edge"
        },
        "method": {
          "enum": [
            "CRISPR enhancer perturbation screens"
          ]
        },
        "class": {
          "enum": [
            "observed data"
          ]
        },
        "label": {
          "enum": [
            "regulatory element effect on gene expression"
          ]
        },
        "source": {
          "enum": [
            "ENCODE"
          ]
        },
        "source_url": {
          "enum": [
            "https://www.encodeproject.org/files/ENCFF968BZL/"
          ]
        },
        "files_filesets": {
          "type": "string",
          "description": "The filesets of the genomic element to gene edge",
          "enum": [
            "files_filesets/ENCFF968BZL"
          ]
        },
        "biological_context": {
          "type": "string",
          "description": "The biological context of the genomic element to gene edge",
          "enum": [
            "K562"
          ]
        },
        "biosample_term": {
          "type": "string",
          "description": "The biosample term of the genomic element to gene edge",
          "enum": [
            "ontology_terms/EFO_0002067"
          ]
        },
        "name": {
          "enum": [
            "regulates"
          ]
        },
        "inverse_name": {
          "enum": [
            "regulated by"
          ]
        }
      },
      "required": [
        "_key",
        "score",
        "p_value",
        "log10pvalue",
        "significant",
        "files_filesets",
        "biological_context",
        "biosample_term"
      ],
      "additionalProperties": false

    }
  ],
  "accessible_via": {
    "filter_by_range": "score",
    "return": "_id, score, source, source_url, file_accession, biological_context, significant"
  },
  "db_collection_name": "genomic_elements_genes"
}
