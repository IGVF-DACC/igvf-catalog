{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/coding_variants_phenotypes.VAMPAdapter.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the coding variant to phenotype edge"
        },
        "_from": {
          "example": "coding_variants/coding_var_1",
          "pattern": "^coding_variants/",
          "collections": [
            "coding_variants"
          ]
        },
        "_to": {
          "example": "ontology_terms/phenotype_1",
          "pattern": "^ontology_terms/",
          "collections": [
            "ontology_terms"
          ]
        },
        "source": {
          "description": "Source for the coding variant to phenotype edge",
          "enum": [
            "IGVF"
          ]
        },
        "source_url": {
          "description": "Source URL for the coding variant to phenotype edge",
          "example": "https://data.igvf.org/tabular-files/IGVFFI0332UGDD",
          "pattern": "^https://data.igvf.org/tabular-files/"
        },
        "name": {
          "description": "Name for the coding variant to phenotype edge",
          "enum": [
            "mutational effect"
          ]
        },
        "inverse_name": {
          "description": "Inverse name for the coding variant to phenotype edge",
          "enum": [
            "altered due to mutation"
          ]
        },
        "files_filesets": {
          "type": "string",
          "description": "Filesets for the coding variant to phenotype edge",
          "example": "files_filesets/IGVFFI0332UGDD",
          "pattern": "^files_filesets/",
          "collections": [
            "files_filesets"
          ]
        },
        "biological_context": {
          "type": "string",
          "description": "The biological context of the coding variant to phenotype edge",
          "items": {
            "type": "string"
          },
          "example": "HAP-1"
        },
        "biosample_term": {
          "type": "string",
          "description": "The biosample term of the coding variant to phenotype edge",
          "items": {
            "type": "string"
          },
          "example": "ontology_terms/EFO_0002067"
        },
        "method": {
          "enum": [
            "VAMP-seq"
          ]
        },
        "label": {
          "enum": [
            "mutational effect"
          ]
        },
        "class": {
          "enum": [
            "observed data"
          ]
        },
        "score": {
          "type": [
            "number",
            "null"
          ],
          "description": "Score for the coding variant to phenotype edge"
        },
        "standard_error": {
          "type": [
            "number",
            "null"
          ],
          "description": "Standard error for the coding variant to phenotype edge"
        },
        "rep1_score": {
          "type": [
            "number",
            "null"
          ],
          "description": "Rep1 score for the coding variant to phenotype edge"
        },
        "rep2_score": {
          "type": [
            "number",
            "null"
          ],
          "description": "Rep2 score for the coding variant to phenotype edge"
        },
        "rep3_score": {
          "type": [
            "number",
            "null"
          ],
          "description": "Rep3 score for the coding variant to phenotype edge"
        }
      },
      "required": [
        "_key",
        "_from",
        "_to",
        "source",
        "source_url",
        "name",
        "inverse_name",
        "files_filesets",
        "simple_sample_summaries",
        "method",
        "biological_context"
      ],
      "additionalProperties": false
    }
  ],
  "db_collection_name": "coding_variants_phenotypes"
}
