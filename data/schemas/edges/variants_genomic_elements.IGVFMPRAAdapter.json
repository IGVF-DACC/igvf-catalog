{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "schemas/edges/variants_genomic_elements.IGVFMPRAAdapter.json",
  "allOf": [
    {
      "$ref": "edges.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "type": "string",
          "description": "Unique identifier for the edge"
        },
        "_from": {
          "example": "variants/NC_000009.12:136248440:T:C",
          "pattern": "^variants/",
          "collections": [
            "variants"
          ]
        },
        "_to": {
          "example": "genomic_elements/MPRA_chr9_136886228_136886428_GRCh38_IGVFFI4914OUJH",
          "pattern": "^genomic_elements/",
          "collections": [
            "genomic_elements"
          ]
        },
        "bed_score": {
          "type": "number",
          "description": "Bed score for the edge"
        },
        "activity_score": {
          "type": "number",
          "description": "Activity score for the edge"
        },
        "DNA_count_ref": {
          "type": "number",
          "description": "DNA count ref for the edge"
        },
        "RNA_count_ref": {
          "type": "number",
          "description": "RNA count ref for the edge"
        },
        "DNA_count_alt": {
          "type": "number",
          "description": "DNA count alt for the edge"
        },
        "RNA_count_alt": {
          "type": "number",
          "description": "RNA count alt for the edge"
        },
        "minusLog10PValue": {
          "type": "number",
          "description": "Minus log10 P value for the edge"
        },
        "minusLog10QValue": {
          "type": "number",
          "description": "Minus log10 Q value for the edge"
        },
        "postProbEffect": {
          "type": "number",
          "description": "Post probability effect for the edge"
        },
        "CI_lower_95": {
          "type": "number",
          "description": "CI lower 95 for the edge"
        },
        "CI_upper_95": {
          "type": "number",
          "description": "CI upper 95 for the edge"
        },
        "class": {
          "enum": [
            "observed data"
          ]
        },
        "label": {
          "enum": [
            "variant effect on regulatory element activity"
          ]
        },
        "name": {
          "enum": [
            "modulates regulatory activity of"
          ]
        },
        "inverse_name": {
          "enum": [
            "regulatory activity modulated by"
          ]
        },
        "method": {
          "enum": [
            "lentiMPRA"
          ]
        },
        "source": {
          "enum": [
            "IGVF"
          ]
        },
        "source_url": {
          "example": "https://data.igvf.org/tabular-files/IGVFFI1323RCIE"
        },
        "files_filesets": {
          "type": "string",
          "description": "Filesets for the edge",
          "example": "files_filesets/IGVFFI1323RCIE",
          "pattern": "^files_filesets/",
          "collections": [
            "files_filesets"
          ]
        },
        "biological_context": {
          "type": "string",
          "description": "Biological contextfor the edge",
          "example": "glutamatergic neuron differentiated cell specimen, pooled cell specimen from IGVFDO6638HIAD"
        },
        "biosample_term": {
          "type": "string",
          "description": "Biosample term for the edge",
          "example": "ontology_terms/UBERON_0002048"
        },
        "treatments_term_ids": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          },
          "description": "Treatments term ids for the edge"
        }
      },
      "required": [
        "_key",
        "label",
        "files_filesets",
        "biosample_term",
        "biological_context",
        "treatments_term_ids"
      ],
      "additionalProperties": false
    }
  ],
  "db_collection_name": "variants_genomic_elements"
}
