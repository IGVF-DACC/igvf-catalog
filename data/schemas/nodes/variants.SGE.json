{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/nodes/variants.SGE.json",
    "allOf": [
        {
            "$ref": "node.base.json"
        },
        {
            "$ref": "variants.base.json"
        },
        {
            "type": "object",
            "properties": {
                "source": {
                    "enum": [
                        "IGVF"
                    ]
                },
                "source_url": {
                    "example": "https://data.igvf.org/tabular-files/IGVFFI0332UGDD",
                    "pattern": "^https://data.igvf.org/tabular-files/"
                },
                "files_filesets": {
                    "type": "string",
                    "description": "Filesets of the variant",
                    "example": "files_filesets/IGVFFI0332UGDD",
                    "pattern": "^files_filesets/",
                    "collections": [
                        "files_filesets"
                    ]
                }
            },
            "required": [
                "rsid",
                "qual",
                "filter",
                "annotations",
                "format",
                "vrs_digest",
                "ca_id",
                "files_filesets"
            ],
            "additionalProperties": false
        }
    ]
}
