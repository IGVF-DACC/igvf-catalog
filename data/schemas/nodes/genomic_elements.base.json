{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "schemas/nodes/genomic_elements.base.json",
  "type": "object",
  "properties": {
    "chr": {
      "type": "string",
      "description": "Chromosome of the genomic element"
    },
    "start": {
      "type": "number",
      "description": "Start position of the genomic element"
    },
    "end": {
      "type": "number",
      "description": "End position of the genomic element"
    },
    "type": {
      "type": "string",
      "description": "Type of the genomic element"
    },
    "method": {
      "type": "string",
      "description": "Method of the genomic element"
    }
  },
  "required": [
    "chr",
    "start",
    "end",
    "type",
    "method"
  ]
}
