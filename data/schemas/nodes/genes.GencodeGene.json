{
    "id": "schemas/nodes/genes.GencodeGene.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "The unique ENSEMBL gene ID",
            "example": "ENSG00000290825",
            "pattern": "^ENSG[0-9]{11}(?:_PAR_Y)?$"
        },
        "gene_id": {
            "type": "string",
            "description": "The unique ENSEMBL gene ID with version number",
            "example": "ENSG00000290825.1",
            "pattern": "^ENSG[0-9]{11}\\.[0-9]+(?:_PAR_Y)?$"
        },
        "gene_type": {
            "type": "string",
            "description": "The type of gene",
            "example": "lncRNA"
        },
        "chr": {
            "type": "string",
            "description": "The chromosome name",
            "example": "chr9",
            "pattern": "^chr"
        },
        "start": {
            "type": "number",
            "description": "The start position of the gene",
            "example": 11869
        },
        "end": {
            "type": "number",
            "description": "The end position of the gene",
            "example": 14409
        },
        "strand": {
            "type": "string",
            "description": "The strand of the gene",
            "enum": ["+", "-"]
        },
        "name": {
            "type": "string",
            "description": "The name of the gene",
            "example": "DDX11L2"
        },
        "symbol": {
            "type": "string",
            "description": "The symbol of the gene",
            "example": "DDX11L2"
        },
        "source": {
            "type": "string",
            "description": "The source of the gene",
            "enum": ["GENCODE"]
        },
        "version": {
            "type": "string",
            "description": "The version of the gene annotation",
            "enum": ["v43"]
        },
        "source_url": {
            "type": "string",
            "description": "The source URL of the gene annotation",
            "enum": ["https://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_43/gencode.v43.chr_patch_hapl_scaff.annotation.gtf.gz"]
        },
        "organism": {
            "type": "string",
            "description": "The organism of the gene",
            "enum": ["Homo sapiens"]
        },
        "study_sets": {
            "type": ["array", "null"],
            "description": "The study sets of the gene",
            "items": {
                "type": "string"
            }
        },
        "collections": {
            "type": ["array", "null"],
            "description": "The collections of the gene",
            "items": {
                "type": "string"
            }
        },
        "version_number": {
            "type": "string",
            "description": "The version number of the ENSEMBL gene ID",
            "example": "1"
        },
        "hgnc": {
            "type": "string",
            "description": "The HGNC ID of the gene",
            "example": "HGNC:100000",
            "pattern": "^HGNC:[0-9]+$"
        },
        "synonyms": {
            "type": "array",
            "description": "The synonyms of the gene",
            "items": {
                "type": "string"
            }
        },
        "entrez": {
            "type": "string",
            "description": "The Entrez ID of the gene",
            "example": "ENTREZ:100287102",
            "pattern": "^ENTREZ:[0-9]+$"
        }
    },
    "required": [
        "_key",
        "gene_id",
        "gene_type",
        "chr",
        "start",
        "end",
        "strand",
        "name",
        "symbol",
        "source",
        "version",
        "source_url",
        "organism",
        "study_sets",
        "collections",
        "version_number"
    ],
    "additionalProperties": false
}
