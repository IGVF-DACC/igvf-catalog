{
  "id": "schemas/nodes/transcripts.Gencode.json",
  "allOf": [
    {
      "$ref": "node.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "pattern": "^ENST[0-9]{11}(?:_PAR_Y)?$",
          "example": "ENST00000456328"
        },
        "transcript_id": {
          "type": "string",
          "description": "The unique ENSEMBL transcript ID with version number",
          "example": "ENST00000456328.2",
          "pattern": "^ENST[0-9]{11}\\.[0-9]+(?:_PAR_Y)?$"
        },
        "name": {
          "example": "DDX11L2-202"
        },
        "transcript_type": {
          "type": "string",
          "description": "The type of transcript",
          "example": "lncRNA"
        },
        "chr": {
          "type": "string",
          "description": "The chromosome name",
          "example": "chr9",
          "pattern": "^chr"
        },
        "start": {
          "type": "number",
          "description": "The start position of the transcript",
          "example": 11869
        },
        "end": {
          "type": "number",
          "description": "The end position of the transcript",
          "example": 14409
        },
        "strand": {
          "type": "string",
          "description": "The strand of the transcript",
          "enum": [
            "+",
            "-"
          ]
        },
        "gene_name": {
          "type": "string",
          "description": "The name of the gene",
          "example": "DDX11L2"
        },
        "source": {
          "enum": [
            "GENCODE"
          ]
        },
        "version": {
          "type": "string",
          "description": "The version of the transcript annotation",
          "enum": [
            "v43"
          ]
        },
        "source_url": {
          "enum": [
            "https://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_43/gencode.v43.chr_patch_hapl_scaff.annotation.gtf.gz"
          ]
        },
        "organism": {
          "type": "string",
          "description": "The organism of the transcript",
          "enum": [
            "Homo sapiens"
          ]
        }
      },
      "required": [
        "transcript_id",
        "transcript_type",
        "chr",
        "start",
        "end",
        "strand",
        "gene_name",
        "version",
        "organism"
      ]
    }
  ],
  "additionalProperties": false
}
