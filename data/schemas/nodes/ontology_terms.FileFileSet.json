{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/nodes/ontology_terms.FileFileSet.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "Unique identifier for the ontology term",
            "example": "CL_0000746"
        },
        "uri": {
            "type": "string",
            "description": "URI for the ontology term",
            "example": "https://www.encodeproject.org/biosample-types/in_vitro_differentiated_cells_NTR_0000856/"
        },
        "name": {
            "type": "string",
            "description": "Name for the ontology term",
            "example": "cardiac muscle cell"
        },
        "synonyms": {
            "type": ["array", "null"],
            "description": "Synonyms for the ontology term"
        },
        "source": {
            "type": "string",
            "description": "Source for the ontology term",
            "enum": ["IGVF", "ENCODE"]
        }
    },
    "required": ["_key", "uri", "name", "synonyms", "source"],
    "additionalProperties": false
}
