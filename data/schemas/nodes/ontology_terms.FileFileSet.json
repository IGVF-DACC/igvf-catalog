{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "id": "schemas/nodes/ontology_terms.FileFileSet.json",
  "allOf": [
    {
      "$ref": "node.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "example": "CL_0000746"
        },
        "name": {
          "example": "cardiac muscle cell"
        },
        "uri": {
          "type": "string",
          "description": "URI for the ontology term",
          "example": "https://www.encodeproject.org/biosample-types/in_vitro_differentiated_cells_NTR_0000856/"
        },
        "term_id": {
          "type": "string",
          "description": "Term ID for the ontology term",
          "example": "CL_0000746"
        },
        "synonyms": {
          "type": [
            "array",
            "null"
          ],
          "description": "Synonyms for the ontology term"
        },
        "source": {
          "enum": [
            "IGVF",
            "ENCODE"
          ]
        },
        "source_url": {
          "example": "https://api.data.igvf.org/sample-terms/CL_0000746"
        }
      },
      "required": [
        "uri",
        "term_id",
        "synonyms"
      ],
      "additionalProperties": false
    }
  ]
}
