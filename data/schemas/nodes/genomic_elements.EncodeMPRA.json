{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/nodes/genomic_elements.EncodeMPRA.json",
    "type": "object",
    "properties": {
        "_key": {
            "type": "string",
            "description": "Unique identifier for the genomic element",
            "example": "MPRA_chr1_10410_10610_GRCh38_ENCFF802FUV"
        },
        "name": {
            "type": "string",
            "description": "Name for the genomic element",
            "example": "MPRA_chr1_10410_10610_GRCh38_ENCFF802FUV"
        },
        "chr": {
            "type": "string",
            "description": "Chromosome for the genomic element",
            "example": "chr1"
        },
        "start": {
            "type": "number",
            "description": "Start position for the genomic element"
        },
        "end": {
            "type": "number",
            "description": "End position for the genomic element"
        },
        "method": {
            "type": "string",
            "description": "Method for the genomic element",
            "example": "MPRA"
        },
        "type": {
            "type": "string",
            "description": "Type for the genomic element",
            "enum": ["tested elements"]
        },
        "source": {
            "type": "string",
            "description": "Source for the genomic element",
            "enum": ["ENCODE"]
        },
        "source_url": {
            "type": "string",
            "description": "Source URL for the genomic element",
            "example": "https://www.encodeproject.org/files/ENCFF802FUV/",
            "pattern": "^https://www.encodeproject.org/files/"
        },
        "files_filesets": {
            "type": "string",
            "description": "Filesets for the genomic element",
            "example": "files_filesets/ENCFF802FUV",
            "pattern": "^files_filesets/"
        }
    },
    "required": ["_key", "name", "chr", "start", "end", "method", "type", "source", "source_url", "files_filesets"],
    "additionalProperties": false
}
