{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "id": "schemas/nodes/drugs.PharmGKB.json",
  "allOf": [
    {
      "$ref": "node.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "example": "PA166238901"
        },
        "name": {
          "example": "17-alpha-dihydroequilenin sulfate"
        },
        "drug_ontology_terms": {
          "type": "array",
          "description": "The drug ontology terms of the drug",
          "example": [
            "ontology_terms/CHEBI_15956"
          ],
          "items": {
            "type": "string",
            "pattern": "^ontology_terms/[A-Z0-9_]+$"
          }
        },
        "source": {
          "enum": [
            "pharmGKB"
          ]
        },
        "source_url": {
          "pattern": "^https://www\\.pharmgkb\\.org/chemical/[A-Z0-9]+$",
          "example": "https://www.pharmgkb.org/chemical/PA166238901"
        }
      },
      "required": [
        "drug_ontology_terms"
      ]
    }
  ],
  "additionalProperties": false
}
