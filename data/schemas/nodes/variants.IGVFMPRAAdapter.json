{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/nodes/variants.IGVFMPRAAdapter.json",
    "allOf": [
        {
            "$ref": "variants.base.json"
        },
        {
            "type": "object",
            "properties": {
                "source": {
                    "type": "string",
                    "description": "Source for the variant",
                    "enum": ["IGVF"]
                },
                "source_url": {
                    "type": "string",
                    "description": "Source URL for the variant",
                    "example": "https://data.igvf.org/tabular-files/IGVFFI1323RCIE",
                    "pattern": "^https://data.igvf.org/tabular-files/"
                },
                "files_filesets": {
                    "type": "string",
                    "description": "Filesets of the variant",
                    "example": "files_filesets/IGVFFI1323RCIE",
                    "pattern": "^files_filesets/",
                    "collections": ["files_filesets"]
                }
            },
            "required": ["rsid", "qual", "filter", "annotations", "format", "vrs_digest", "ca_id", "files_filesets"]
        }
    ]
}
