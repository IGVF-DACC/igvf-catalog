{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "id": "schemas/nodes/coding_variants.ESM1vCodingVariantsScores.json",
  "allOf": [
    {
      "$ref": "node.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "example": "DSG2_ENST00000261590_p.Gln873Thr_c.2617_2618delinsAC"
        },
        "name": {
          "example": "DSG2_ENST00000261590_p.Gln873Thr_c.2617_2618delinsAC"
        },
        "ref": {
          "type": "string",
          "description": "Reference amino acid for the coding variant",
          "example": "Q"
        },
        "alt": {
          "type": "string",
          "description": "Alternative amino acid for the coding variant",
          "example": "T"
        },
        "aapos": {
          "type": "integer",
          "description": "Amino acid position for the coding variant"
        },
        "refcodon": {
          "type": "string",
          "description": "Reference codon for the coding variant",
          "example": "ATG"
        },
        "gene_name": {
          "type": "string",
          "description": "Gene name for the coding variant",
          "example": "DSG2"
        },
        "protein_name": {
          "type": "string",
          "description": "Protein name for the coding variant",
          "example": "A0A2U3U0J3_HUMAN"
        },
        "protein_id": {
          "type": "string",
          "description": "Protein ENSEMBL id for the coding variant",
          "example": "ENSP00000493376"
        },
        "codonpos": {
          "type": "integer",
          "description": "Codon position for the coding variant"
        },
        "hgvsc": {
          "type": "string",
          "description": "HGVS for the coding variant",
          "example": "c.2617_2618delinsAC"
        },
        "hgvsp": {
          "type": "string",
          "description": "HGVS for the coding variant",
          "example": "p.Gln873Thr",
          "pattern": "^p."
        },
        "transcript_id": {
          "type": "string",
          "description": "Transcript ENSEMBL id for the coding variant",
          "example": "ENST00000261590"
        },
        "source": {
          "enum": [
            "IGVF"
          ]
        },
        "source_url": {
          "example": "https://data.igvf.org/tabular-files/IGVFFI8105TNNO",
          "pattern": "^https://data.igvf.org/tabular-files/"
        }
      },
      "required": [
        "ref",
        "alt",
        "aapos",
        "refcodon",
        "gene_name",
        "protein_name",
        "protein_id",
        "codonpos",
        "hgvsc",
        "hgvsp",
        "transcript_id"
      ],
      "additionalProperties": false
    }
  ]
}
