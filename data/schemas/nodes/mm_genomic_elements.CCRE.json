{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "schemas/nodes/mm_genomic_elements.CCRE.json",
    "allOf": [
      {
        "$ref": "node.base.json"
      },
      {
        "$ref": "genomic_elements.base.json"
      },
      {
        "type": "object",
        "properties": {
          "source_annotation": {
            "type": "string",
            "description": "Source annotation of the CCRE",
            "example": "CA-CTCF: chromatin accessible + CTCF binding"
          },
          "method": {
            "enum": [
              "candidate Cis-Regulatory Elements"
            ]
          },
          "type": {
            "enum": [
              "candidate cis regulatory element"
            ]
          },
          "source": {
            "enum": [
              "ENCODE"
            ]
          },
          "source_url": {
            "enum": [
              "https://www.encodeproject.org/files/ENCFF167FJQ"
            ]
          },
          "files_filesets": {
            "type": "string",
            "description": "Filesets of the CCRE",
            "enum": [
              "files_filesets/ENCFF167FJQ"
            ]
          }
        },
        "required": [
          "source_annotation",
          "files_filesets"
        ],
        "additionalProperties": false
      }
    ],
    "accessible_via": {
      "filter_by_range": "start, end",
      "return": "_id, name, chr, start, end, type, source_annotation, type, source, source_url"
    },
    "db_collection_name": "mm_genomic_elements"
  }
