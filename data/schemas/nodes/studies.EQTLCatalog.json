{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "id": "schemas/nodes/studies.EQTLCatalog.json",
  "allOf": [
    {
      "$ref": "node.base.json"
    },
    {
      "type": "object",
      "properties": {
        "_key": {
          "example": "QTS000038"
        },
        "name": {
          "example": "OneK1K"
        },
        "pmid": {
          "type": "string",
          "description": "The PMID of the study",
          "example": "35389779"
        },
        "study_type": {
          "type": "string",
          "description": "The type of the study",
          "enum": [
            "single-cell",
            "bulk"
          ]
        },
        "source": {
          "enum": [
            "eQTL Catalogue"
          ]
        },
        "source_url": {}
      },
      "required": [
        "pmid",
        "study_type"
      ],
      "additionalProperties": false
    }
  ]
}
