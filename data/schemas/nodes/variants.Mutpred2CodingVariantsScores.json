{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/nodes/variants.Mutpred2CodingVariantsScores.json",
    "allOf": [
        {
            "$ref": "variants.base.json"
        },
        {
            "type": "object",
            "properties": {
                "variation_type": {
                    "type": "string",
                    "description": "Variation type for the variant",
                    "enum": ["SNP", "deletion-insertion"]
                },
                "source": {
                    "type": "string",
                    "description": "Source of the variant",
                    "enum": ["IGVF"]
                },
                "source_url": {
                    "type": "string",
                    "description": "Source URL of the variant",
                    "example": "https://data.igvf.org/tabular-files/IGVFFI0332UGDD",
                    "pattern": "^https://data.igvf.org/tabular-files/"
                },
                "files_filesets": {
                    "type": "string",
                    "description": "Filesets of the variant",
                    "example": "files_filesets/IGVFFI0332UGDD",
                    "pattern": "^files_filesets/",
                    "collections": ["files_filesets"]
                }
            }
        }
    ],
    "required": ["files_filesets"]
}
