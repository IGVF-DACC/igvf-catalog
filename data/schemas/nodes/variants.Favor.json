{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "schemas/nodes/variants.Favor.json",
    "allOf": [
        {
            "$ref": "variants.base.json"
        },
        {
            "type": "object",
            "properties": {
                "source": {
                    "type": "string",
                    "description": "Source of the variant",
                    "enum": ["FAVOR"]
                },
                "source_url": {
                    "type": "string",
                    "description": "Source URL of the variant",
                    "example": "http://favor.genohub.org/"
                }
            },
            "required": ["rsid", "qual", "filter", "annotations", "format", "vrs_digest", "ca_id"]
        }
    ]
}
