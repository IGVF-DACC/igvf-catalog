{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "allOf": [
    {
      "$ref": "node.base.json"
    },
    {
      "type": "object",
      "properties": {
        "chr": {
          "type": "string",
          "description": "Chromosome for the genomic element"
        },
        "start": {
          "type": "number",
          "description": "Start position for the genomic element"
        },
        "end": {
          "type": "number",
          "description": "End position for the genomic element"
        },
        "method": {
          "type": "string",
          "description": "Method for the genomic element",
          "enum": [
            "integrative"
          ]
        },
        "type": {
          "type": "string",
          "description": "Type for the genomic element",
          "enum": [
            "accessible dna elements (mouse)"
          ]
        },
        "source": {
          "enum": [
            "FUNCODE"
          ]
        },
        "source_url": {
          "pattern": "^https://data.igvf.org/reference-files/"
        }
      },
      "required": [
        "chr",
        "start",
        "end",
        "method",
        "type"
      ]
    }
  ],
  "$id": "schemas/nodes/mm_genomic_elements.HumanMouseElementAdapter.json",
  "additionalProperties": false
}
